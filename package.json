{
  "name": "umijs-react",
  "version": "0.1.0",
  "private": true,
  "author": "wowo",
  "scripts": {
    "dev": "max dev",
    "format": "prettier --cache --write src",
    "lint": "eslint src --fix --ext .ts,.tsx,.js,.jsx",
    "build": "max build",
    "postinstall": "max setup",
    "setup": "max setup",
    "start": "npm run dev",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/pro-components": "^2.8.10",
    "@umijs/max": "^4.6.25",
    "antd": "^6.2.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-compiler-runtime": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "husky": "^9",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,less}": [
      "prettier --write"
    ]
  }
}